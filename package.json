{
  "name": "crds-components",
  "version": "1.1.1",
  "description": "Custom Elements for Crossroads Church",
  "module": "dist/index.mjs",
  "main": "dist/crds-components.js",
  "repository": "git://github.com/crdschurch/crds-components",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "npm run crds-ip:test && stencil build --docs",
    "clean": "rimraf dist www",
    "deploy": "echo 'deploy'",
    "start": "stencil build --dev --watch --serve",
    "storybook": "start-storybook -p 9009 -s www",
    "storybook:build": "build-storybook",
    "storybook:watch": "run-p start storybook",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "graphql-codegen",
    "snapshot": "build-storybook && percy-storybook --widths=320,1280",
    "cypress:open": "stencil build && concurrently 'npm run storybook:run' 'npm run cypress:test:open' -k -s first",
    "cypress:run": "stencil build && concurrently 'npm run storybook:run' 'npm run cypress:test:run' -k -s first",
    "storybook:run": "start-storybook -p 9009",
    "cypress:test:open": "wait-on http://localhost:9009 && cypress open",
    "cypress:test:run": "wait-on http://localhost:9009 && cypress run",
    "crds-ip:test": "npm run cypress:run && npm run snapshot"
  },
  "devDependencies": {
    "@alepop/stencil-env": "^0.2.0",
    "@babel/core": "^7.7.2",
    "@crds_npm/crds-client-auth": "^1.1.18",
    "@graphql-codegen/cli": "^1.8.3",
    "@graphql-codegen/typescript": "^1.8.3",
    "@percy/cypress": "^2.2.0",
    "@percy/storybook": "^3.2.0",
    "@stencil/core": "1.8.1",
    "@stencil/sass": "1.1.1",
    "@storybook/addon-knobs": "^5.2.6",
    "@storybook/addon-storysource": "^5.2.6",
    "@storybook/polymer": "^5.2.6",
    "@types/jest": "^24.0.23",
    "@types/node": "^10.17.5",
    "@types/puppeteer": "1.19.1",
    "acorn": "^6.3.0",
    "ajv": "^6.10.2",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.2",
    "crds-styles": "3.3.6",
    "cypress": "^3.8.2",
    "cypress-shadow-dom": "^1.3.0",
    "iframe-resizer": "^4.2.8",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "lit-html": "^1.1.2",
    "marked": "^0.7.0",
    "node-sass-magic-importer": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "object-dig": "^0.1.3",
    "polymer-webpack-loader": "^2.0.3",
    "prettier": "^1.19.1",
    "prettier-stylelint": "^0.4.2",
    "puppeteer": "1.19.0",
    "rimraf": "^2.7.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-replace": "^2.2.0",
    "stencil-fragment": "^1.0.1",
    "stencil-inline-svg": "^1.0.1",
    "stylelint": "^10.1.0",
    "stylelint-formatter-pretty": "^1.1.3",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.12.1",
    "unescape": "^1.0.1",
    "url": "^0.11.0",
    "wait-on": "^4.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "apollo-boost": "^0.4.4",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-context": "^1.0.19",
    "apollo-link-http": "^1.5.16",
    "crds-cypress-login": "^1.0.1",
    "detect-node": "^2.0.4",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "jquery": "^3.4.1",
    "node-fetch": "^2.6.0",
    "popper.js": "^1.16.0",
    "toastr": "^2.1.4"
  }
}
